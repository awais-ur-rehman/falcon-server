{
  "name": "iae-platform-backend",
  "version": "1.0.0",
  "description": "Backend API for IAE Platform authentication system with MongoDB",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "nodemon --exec tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "docker": "tsx ./src/index.ts",
    "swagger": "node ./src/swagger.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "authentication",
    "jwt",
    "mongodb",
    "mongoose",
    "api",
    "typescript"
  ],
  "author": "Awais",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^9.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "rimraf": "^5.0.10",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
